{
  "name": "@foxglove/schemas",
  "version": "2.0.0",
  "description": "Foxglove-defined message schemas for ROS, Protobuf, FlatBuffers, OMG IDL, and JSON",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Foxglove",
    "email": "support@foxglove.dev",
    "url": "https://foxglove.dev/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxglove/foxglove-sdk.git",
    "directory": "typescript/schemas"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./internal": {
      "types": "./dist/internal/index.d.ts",
      "import": "./dist/internal/index.js"
    },
    "./jsonschema": {
      "types": "./dist/jsonschema/index.d.ts",
      "import": "./dist/jsonschema/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "clean": "git clean -Xdf",
    "build": "cp -R ../../schemas/jsonschema ./src/jsonschema && tsc -b",
    "prepack": "yarn clean && yarn build"
  },
  "dependencies": {
    "@foxglove/rosmsg-msgs-common": "^3.2.2"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "5.9.2"
  }
}
